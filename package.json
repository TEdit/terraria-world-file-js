{
  "name": "terraria-world-parser",
  "version": "2.0.0",
  "description": "Terraria world file parser in javascript",
  "main": "src/node/terraria-world-parser.js",
  "scripts": {
    "test:node": "node src/tests/node.js",
    "test:browser": "parcel serve ./examples/map-viewer-browser.html --port 80 --open --out-dir ./test --cache-dir ./test/.cache --out-file ./test/browser-built.html --target browser",
    "compile:browser": "parcel build ./src/browser/terraria-world-parser.js --out-file ./twp.min.js --no-source-maps --no-content-hash --no-cache --experimental-scope-hoisting"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cokolele/terraria-world-parser.git"
  },
  "keywords": [
    "terraria",
    "world file",
    "file structure",
    "file dumper",
    "file format",
    "documentation",
    "data",
    "parsing",
    "parser",
    "map viewer",
    "tool",
    "javascript",
    "node",
    "browser"
  ],
  "author": "cokolele",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cokolele/terraria-world-parser/issues"
  },
  "homepage": "https://github.com/cokolele/terraria-world-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "core-js": "^2.5.7",
    "parcel-bundler": "^1.12.4",
    "regenerator-runtime": "^0.13.3"
  }
}
